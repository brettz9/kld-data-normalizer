type Ellipse {
    center {
        center: Point2D, [Number;2]
        group {
            centerX, cx: Number
            centerY, cy: Number
        }
    }
    radii {
        radii: Vector2D, [Number;2]
        group {
            radiusX, rx: Number
            radiusY, ry: Number
        }
    }
}
